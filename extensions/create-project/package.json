{
  "name": "create-project",
  "displayName": "letter-创建项目",
  "description": "测试",
  "publisher": "letterletter",
  "icon": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    ""
  ],
  "activationEvents": [
    "onCommand:.hello"
  ],
  "main": "./build/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/apptools-lab/appworks.git"
  },
  "bugs": {
    "url": "https://github.com/apptools-lab/appworks/issues"
  },
  "scripts": {
    "vscode:prepublish": "rm -rf build && npm run build:web && npm run build:extension",
    "preinstall": "cd web && npm install",
    "build:web": "cd web && npm run build",
    "watch:web": "cd web && npm run start",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "build:extension": "webpack --mode production",
    "watch:extension": "webpack --mode development --watch --info-verbosity verbose",
    "compile": "tsc -p ./tsconfig.json"
  },
  "contributes": {
    "commands": [
      {
        "command": ".hello",
        "title": "%commands.hello.title%"
      }
    ]
  },
  "dependencies": {
    "@appworks/common-service": "0.1.0",
    "@appworks/project-service": "0.1.0",
    "@appworks/connector": "0.1.0"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.73.0",
    "typescript": "^4.5.2",
    "ts-loader": "^7.0.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
